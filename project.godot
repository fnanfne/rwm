config_version=5

[application]

config/name="Robot Wants Music"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.0", "Mobile")
boot_splash/image="res://Assets/Pics/New RwK Logo_.png"
config/icon="res://Assets/Pics/Kitty.png"
config/windows_native_icon="res://Assets/Icons/kitty.ico"

[autoload]

Game="*res://Global/Game.gd"
Utils="*res://Global/Utils.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://Assets/Pics/robot_arm.png"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/sound_manager/plugin.cfg")

